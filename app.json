{
  "expo": {
    "name": "UKM Loby RoboCar",
    "slug": "NativeRobocar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ukmloby.nativerobocar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptasi.png",
        "backgroundColor": "#111827"
      },
      "package": "com.ukmloby.nativerobocar"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    }
  }
}
